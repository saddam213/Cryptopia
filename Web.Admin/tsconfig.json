{
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "redux",
      "react-redux"
    ],
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "target": "es5",
    "jsx": "react",
    "outDir": "compiledts"
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
